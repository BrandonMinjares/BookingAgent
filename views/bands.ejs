<% include partials/header %>

<div class = "container-fluid">
    <div class ="row">
        <form>
            <input type = "text" placeholder="Search for Bands">
        </form>
        <h3>Tributes</h3>
            <% bands.forEach(function(band) { %>
                <div class="col">
                <h4><%= band.name %></h4>
                <img class="image-resize-thumbnail" src="/public/uploads/<%=band.photo %>">
                <p>
                    <a href="/bands/<%= band._id %>" class="btn btn-primary">More Info</a>
                </p>
                </div>
            <% }); %>
    </div>
</div>

<% include partials/footer %>
